{
  "name": "grapesjs-pages",
  "version": "1.0.1",
  "description": "Grapesjs Pages",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/dipaksarkar/grapesjs-pages.git"
  },
  "scripts": {
    "start": "grapesjs-cli serve",
    "build": "grapesjs-cli build",
    "build:css": "sass src/styles.scss dist/grapesjs-pages.min.css --style compressed",
    "watch:scss": "sass --watch src/styles.scss public/grapesjs-pages.min.css --style compressed",
    "bump": "npm version patch -m 'Bump v%s'"
  },
  "keywords": [
    "grapesjs",
    "plugin"
  ],
  "devDependencies": {
    "grapesjs-cli": "^4.1.3",
    "sass": "^1.32.8"
  },
  "license": "MIT",
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "dom-to-image": "^2.6.0"
  }
}
